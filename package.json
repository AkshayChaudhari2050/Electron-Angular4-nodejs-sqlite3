{
  "name": "angular-electron",
  "version": "0.0.0",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng build --prod",
    "serve": "ng serve",
    "build": "node taskapi.js",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "nodemon taskapi.js && electron . ",
    "e": "ng build --prod && electron . ",
    "built": "electron-packager . /myapp --platform=win32 --arch=x64",
    "rebuild": "electron-rebuild -f -w sqlite3",
    "l": "tsc && concurrently \"tsc -w\" \"lite-server\" ",
    "b": "tsc && concurrently \"npm run tsc:w\" \"electron main.js\"",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.2.4",
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/forms": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/router": "^4.2.4",
    "angular-in-memory-web-api": "^0.5.1",
    "angular-svg-round-progressbar": "^1.1.0",
    "asar": "^0.14.0",
    "body-parser": "^1.18.2",
    "bulma": "^0.5.3",
    "concurrently": "^3.5.0",
    "core-js": "^2.4.1",
    "electron-rebuild": "^1.6.0",
