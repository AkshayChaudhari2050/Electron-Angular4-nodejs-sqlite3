    "ng2-bootstrap": "^1.6.3",
    "node-remote-debugger": "^1.1.2",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "rxjs": "^5.4.2",
    "sequelize": "^4.22.5",
    "sqlite3": "^3.1.13",
    "typings": "^2.1.1",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "1.4.1",
